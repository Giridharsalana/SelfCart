{
  "name": "self-cart-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for Self Cart App",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@types/nodemailer": "^7.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.3",
    "razorpay": "^2.8.6",
    "stripe": "^12.9.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
